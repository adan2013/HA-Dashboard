{
  "name": "ha-dashboard",
  "private": true,
  "version": "3.3.0",
  "description": "Home Assistant dashboard for wall-mounted tablet",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabgrel10/vite-react-starter"
  },
  "keywords": [
    "vite",
    "react",
    "home assistant",
    "smart home"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build --emptyOutDir",
    "serve": "vite preview",
    "lint": "eslint ./src --ext .js,.ts,.tsx",
    "lint-fix": "eslint --fix ./src --ext .js,.ts,.tsx",
    "lint:stylelint": "stylelint **/*.{css,scss,tsx}",
    "fix:stylelint": "stylelint --fix  ./src/**/*.{css, scss,tsx}",
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage-full": "jest --coverage --collectCoverageFrom=src/**/*.{ts,tsx}",
    "docker-build": "docker build -t ha-dashboard .",
    "docker-run": "docker run -d -p 3000:80 ha-dashboard"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.6",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/parser": "^5.60.0",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "eventemitter3": "^5.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.3",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.10.0",
    "react-toastify": "^9.1.3",
    "recharts": "^2.7.2",
    "sass": "^1.61.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.11",
    "@emotion/babel-plugin": "^11.10.6",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@namics/stylelint-bem": "^8.1.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.2",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.6.4",
    "postcss": "^8.4.21",
    "postcss-scss": "^4.0.6",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.7",
    "prettier-plugin-style-order": "^0.2.2",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^15.4.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-scss": "^4.6.0",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "webpack": "^5.78.0"
  }
}
